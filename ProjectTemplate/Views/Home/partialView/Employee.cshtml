@model IEnumerable<ProjectTemplate.Models.EmployeeData>
@{
    ViewBag.Title = "View";
}


<table class="table table-striped table-hover table-responsive">
    <thead>
        <tr>

            <th>Srno</th>
            <th>Ation</th>
            <th>Name</th>
            <th>Address</th>
            <th>MobileNo</th>
            <th>Email</th>
            <th class="text-nowrap">Date Of Birth</th>
            <th class="text-nowrap">Department Name</th>
            <th>City Name</th>
            <th>State Name</th>

        </tr>
    </thead>
    <tbody>
        @if (Model.Any())
        {
            int i = 0;
            foreach (var item in Model)
            {
                i++;
        <tr>
            <td>@i</td>
            <td class="row"><a href="#" class="btn btn-info btn-sm editBtn col-sm-6"><i class="fas fa-pen"></i></a><a href="#" class="btn btn-danger col-sm-6 btn-sm delBtn"><i class="fas fa-times"></i></a></td>
            <td>@item.FirstName @item.LastName</td>
            <td>@item.Address</td>
            <td>@item.MobileNo</td>
            <td>@item.EmailId</td>
            <td>@item.DateOfBirth</td>
            <td>@item.DepartmentName</td>
            <td>@item.CityName</td>
            <td>@item.StateName</td>

        </tr>
            }
        }        
    </tbody>
</table>
